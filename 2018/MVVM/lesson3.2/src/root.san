<template>
  <div class='root'>
    <date-picker on-selectDate='setDate'/>
  </div>
</template>

<script>
  import sDatePicker from './components/sDatePicker.js'

  export default {
    components: {
      'date-picker': sDatePicker
    },
    initData() {
      return {
        date: ''
      }
    },
    setDate(date) {
      this.data.set('date', date)
    }
  }
</script>

